# ==========================================
# QA / STAGING ENVIRONMENT (.env.qa)
# ==========================================

# Core
NODE_ENV=production
NEXT_PUBLIC_APP_NAME="TechSonance POS (QA)"
NEXT_PUBLIC_APP_URL="https://qa.techsonance.com"

# Features
NEXT_PUBLIC_ENABLE_ANALYTICS=true
NEXT_PUBLIC_ENABLE_NOTIFICATIONS=true

# Database (QA Cloud DB)
DATABASE_URL="postgres://user:password@qa-db-host:5432/techsonance_pos_qa?sslmode=require"

# Authentication
NEXTAUTH_URL="https://qa.techsonance.com"
NEXTAUTH_SECRET="qa-secure-random-secret-key"
